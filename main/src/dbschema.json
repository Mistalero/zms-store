{
	"db_name": "zmsstore",
	"db_file": "data/zmsstore.db",
	"version": 2,
	"maps": {
		"users/.+/content.json": {
			"to_json_table": ["cert_user_id"],
			"file_name": "data.json"
		},
		"users/.+/data.json": {
			"to_table": [
				{
					"table": "theme",
					"key_col": "title"
				}
			]
		}
	},
	"tables": {
		"json": {
			"cols": [
				["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
				["directory", "TEXT"],
				["file_name", "TEXT"],
				["cert_user_id", "TEXT"]
			],
			"indexes": ["CREATE UNIQUE INDEX path ON json(directory, file_name)"],
			"schema_changed": 5
		},

		"theme": {
			"cols": [
				["title", "TEXT"],
				["zip_name", "TEXT"],
				["screenshot_name", "TEXT"],
				["version", "INTEGER"],
				["json_id", "INTEGER"]
			],
			"indexes": [],
			"schema_changed": 1
		},
		"plugin": {
			"cols": [
				["title", "TEXT"],
				["description", "TEXT"],
				["zip_name", "TEXT"],
				["version", "INTEGER"],
				["json_id", "INTEGER"]
			],
			"indexes": [],
			"schema_changed": 1
		}
	}
}